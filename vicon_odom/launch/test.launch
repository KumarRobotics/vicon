<launch>
  <node pkg="vicon"
    type="vicon"
    name="vicon"
    output="screen">
    <param name="vicon_server" value="alkaline"/>
    <param name="calib_files_dir" value="$(find vicon)/calib"/>
  </node>
  <node pkg="vicon_odom"
    type="vicon_odom"
    name="vicon_odom"
    output="screen">
    <remap from="~vicon" to="/vicon/QuadrotorNovember"/>
    <remap from="~odom" to="/vicon/QuadrotorNovember_odom"/>
  </node>
</launch>
